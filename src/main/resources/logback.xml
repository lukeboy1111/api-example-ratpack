<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>DEMO %d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.lukec" level="DEBUG" />
  
  <root level="DEBUG">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
